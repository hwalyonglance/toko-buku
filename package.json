{
	"name": "ilm",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"analyzer": "webpack-bundle-analyzer dist/stats.json",
		"build:prod": "ng build --prod -sw true -ec true -aot true --stats-json && npm run ngsw && ng build --prod --aot true --app server --output-hashing=none --stats-json",
		"build:prod:experimental": "ng build --prod -sw true -ec true -aot true --stats-json && npm run ngsw && ng build --prod --aot true --app server --output-hashing=none --bundle-dependencies all --stats-json",
		"build": "ng build && npm run generate-sw && ng build --app server --output-hashing=none --bundle-dependencies all",
		"ngsw": "ngsw-config dist/public ./src/ngsw-config.json",
		"postngsw": "cp node_modules/@angular/service-worker/ngsw-worker.js ./dist/public/ngsw-worker.js",
		"tsc": "cd server && tsc",
		"posttsc": "node dist/server.js",
		"server": "node dist/server.js"
	},
	"dependencies": {
		"@angular/animations": "^5.2.1",
		"@angular/cdk": "^5.1.0",
		"@angular/common": "^5.2.1",
		"@angular/compiler": "^5.2.1",
		"@angular/core": "^5.2.1",
		"@angular/flex-layout": "^2.0.0-beta.12",
		"@angular/forms": "^5.2.1",
		"@angular/http": "^5.2.1",
		"@angular/material": "^5.1.0",
		"@angular/platform-browser": "^5.2.1",
		"@angular/platform-browser-dynamic": "^5.2.1",
		"@angular/platform-server": "^5.2.1",
		"@angular/platform-webworker": "^5.2.1",
		"@angular/platform-webworker-dynamic": "^5.2.1",
		"@angular/router": "^5.2.1",
		"@angular/service-worker": "^5.2.1",
		"@nguniversal/common": "^5.0.0-beta.5",
		"@nguniversal/express-engine": "^5.0.0-beta.5",
		"@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
		"angularfire2": "latest",
		"firebase": "latest",
		"animate.css": "latest",
		"classlist.js": "latest",
		"core-js": "latest",
		"express": "latest",
		"hammerjs": "latest",
		"html-pdf-2": "^1.4.1",
		"idb-keyval": "latest",
		"intl": "latest",
		"moment": "latest",
		"multer": "latest",
		"preboot": "latest",
		"reflect-metadata": "*",
		"rxjs": "latest",
		"uuid": "*",
		"web-animations-js": "latest",
		"workbox-build": "latest",
		"workbox-sw": "latest",
		"zone.js": "latest"
	},
	"devDependencies": {
		"@angular/cli": "1.6.5",
		"@angular/compiler-cli": "^5.2.1",
		"@angular/language-service": "^5.2.1",
		"typescript": "2.6.2",
		"webpack": "^3.10.0"
	}
}
